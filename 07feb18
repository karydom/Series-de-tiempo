 ## vector: operador c, concatena elementos o vectores
 
 salarios<-c(4000,5000,55000)
 
 salarios1<-c(4000,5000,55000)
 salarios2<-c(4000,5000,50000)
 
 ## combinando vectores
 
 salarios3<-c(40000,50000,55000)
 salarios4<-c(10000,15000,20000)
 salarios5<-c(salarios3, salarios4)
 
 ##vectores con distinto tipos de datos
 
 alumnos<-c("Elisa", "Bonifacio", "Edgar")
 alumnos.salarios<-c("Elisa","Bonifacio", "Edgar", 40000,50000,55000)
 
 ## Funciones y vectores
 sum(salarios3)
 length(salarios3)
 sum(salarios3)/length(salarios3)
 mean(salarios3)
 
 ## Otras funciones: sort() es ordenar, min(), max(), range()
 
 #varianza
 x<-c(20,22,24,26,28)
 mediax<-mean(x)
 (x-mediax)^2 ## diferencia al cuadrado
 sum((x-mediax)^2) ## suma de diferencias al cuadrado
 n=length(x)
 
 sum((x-mediax)^2)/(n-1)
 var(x)
 mean(x)
 sd(x) #desviacion estandar
 
